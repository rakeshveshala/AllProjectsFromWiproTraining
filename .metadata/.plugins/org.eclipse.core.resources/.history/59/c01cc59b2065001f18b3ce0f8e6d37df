package com.wipro.demo.dao;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.wipro.demo.model.Movie;

/*
 * Crud repository - contains method which can be used
 *  to insert,delete,update, view opearation
 *  
 * Jpa repository-is extending from crudrepository which contains 
 * all the methods of crudrepository as well as contains pagination
 *  and sorting
 */
@Repository
public interface MovieRepository extends JpaRepository<Movie, Integer>{
	

}